{
  "name": "axios-oauth-client",
  "version": "2.2.0",
  "description": "OAuth 2.0 client utils for axios",
  "author": "Jonathon Hill <jhill9693@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && unbuild",
    "prebuild": "npm run test",
    "clean": "rimraf dist",
    "lint": "standard --fix src",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "pretest": "npm run lint",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compwright/axios-oauth-client.git"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "axios",
    "interceptor"
  ],
  "bugs": {
    "url": "https://github.com/compwright/axios-oauth-client/issues"
  },
  "homepage": "https://github.com/compwright/axios-oauth-client#readme",
  "type": "module",
  "engines": {
    "node": ">= 14"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "standard": {
    "env": [
      "jest"
    ]
  },
  "devDependencies": {
    "jest": "*",
    "rimraf": "*",
    "standard": "*",
    "standard-version": "*",
    "unbuild": "*"
  },
  "peerDependencies": {
    "axios": "^1.2.1"
  }
}
